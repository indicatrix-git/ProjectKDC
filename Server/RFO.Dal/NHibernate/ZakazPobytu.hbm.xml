  </sql-query>
  <sql-query name="ZakazPobytuDelete">
    exec FO.ZAKAZ_POBYTU_DEL :ID, :TransakciaZruseneId
  </sql-query>
  <sql-query name="ZakazPobytuFOGet">
    <return class="Ditec.RIS.RFO.Dol.ZakazPobytu"></return>
    exec FO.ZAKAZ_POBYTU_FYZICKA_OSOBA_GET  :FYZICKA_OSOBA_ID
  </sql-query>

</hibernate-mapping>
