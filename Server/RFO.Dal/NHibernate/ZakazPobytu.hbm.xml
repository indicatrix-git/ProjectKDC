<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Ditec.RIS.RFO.Dol" assembly="Ditec.RIS.RFO.Dol">
  <class name="Ditec.RIS.RFO.Dol.ZakazPobytu" table="ZAKAZ_POBYTU">
    <id name="ID" column="ZAKAZ_POBYTU_ID" />
    <property name="FyzickaOsobaId" column="FYZICKA_OSOBA_ID" />
    <property name="RfoObecId" column="RFO_OBEC_ID" />
    <property name="PlatnostOd" column="PLATNOST_OD" />
    <property name="PlatnostDo" column="PLATNOST_DO" />
    <property name="Poznamka" column="POZNAMKA" />
    <property name="Identifikator" column="IDENTIFIKATOR" />
    <property name="TransakciaId" column="TRANSAKCIA_ID" />
    <property name="TransakciaZruseneId" column="TRANSAKCIA_ZRUSENE_ID" />
    <property name="Changed" column="CHANGED" />

  </class>

  <sql-query name="ZakazPobytuGet">
    <return class="Ditec.RIS.RFO.Dol.ZakazPobytu"></return>
    exec FO.ZAKAZ_POBYTU_GET :ID
  </sql-query>
  <sql-query name="ZakazPobytuCreate">
