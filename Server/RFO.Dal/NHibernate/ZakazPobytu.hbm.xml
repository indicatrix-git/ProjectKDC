    exec FO.ZAKAZ_POBYTU_LIST :FyzickaOsobaId, :RfoObecId, :PlatnostOd, :PlatnostDo, :Poznamka, :Identifikator
  </sql-query>
  <sql-query name="ZakazPobytuBrowseCnt">
    exec FO.ZAKAZ_POBYTU_LISTCNT :FyzickaOsobaId, :RfoObecId, :PlatnostOd, :PlatnostDo, :Poznamka, :Identifikator
  </sql-query>
  <sql-query name="ZakazPobytuBrowsePg">
    <return class="Ditec.RIS.RFO.Dol.ZakazPobytu"></return>
    exec FO.ZAKAZ_POBYTU_LISTPG :FyzickaOsobaId, :RfoObecId, :PlatnostOd, :PlatnostDo, :Poznamka, :Identifikator, :CurrentPage, :RecordsPerPage
  </sql-query>
  <sql-query name="ZakazPobytuUpdate">
