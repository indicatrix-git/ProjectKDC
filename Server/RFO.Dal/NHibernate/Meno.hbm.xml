<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Ditec.RIS.RFO.Dol" assembly="Ditec.RIS.RFO.Dol">
  <class name="Ditec.RIS.RFO.Dol.Meno" table="MENO">
    <id name="ID" column="MENO_ID" />
    <property name="FyzickaOsobaId" column="FYZICKA_OSOBA_ID" />
    <property name="Hodnota" column="HODNOTA" />
    <property name="Poradie" column="PORADIE" />
    <property name="TransakciaId" column="TRANSAKCIA_ID" />
    <property name="TransakciaZruseneId" column="TRANSAKCIA_ZRUSENE_ID" />
    <property name="Changed" column="CHANGED" />
    <property name="Identifikator" column="IDENTIFIKATOR" />
  </class>

  <sql-query name="MenoGet">
    <return class="Ditec.RIS.RFO.Dol.Meno"></return>
    exec FO.MENO_GET :ID
  </sql-query>
  <sql-query name="MenoCreate">
    <return class="Ditec.RIS.RFO.Dol.Meno"></return>
    exec FO.MENO_INS :ID, :FyzickaOsobaId, :Hodnota, :Poradie, :TransakciaId, :TransakciaZruseneId, :Changed, :Identifikator
  </sql-query>
  <sql-query name="MenoDelete">
    exec FO.MENO_DEL :ID, :TransakciaZruseneId
  </sql-query>
  <sql-query name="MenoList">
    <return class="Ditec.RIS.RFO.Dol.Meno"></return>
    exec FO.MENO_LIST :FyzickaOsobaId, :Hodnota, :Poradie, :Identifikator
  </sql-query>
  <sql-query name="MenoBrowsePg">
    <return class="Ditec.RIS.RFO.Dol.Meno"></return>
    exec FO.MENO_LISTPG :FyzickaOsobaId, :Hodnota, :Poradie, :Identifikator, :CurrentPage, :RecordsPerPage
  </sql-query>
  <sql-query name="MenoUpdate">
    <return class="Ditec.RIS.RFO.Dol.Meno"></return>
    exec FO.MENO_UPD :ID, :FyzickaOsobaId, :Hodnota, :Poradie, :TransakciaId, :TRANSAKCIA_ID_OLD, :Identifikator
  </sql-query>
  <sql-query name="MenoBrowseCnt">
    exec FO.MENO_LISTCNT :FyzickaOsobaId, :Hodnota, :Poradie, :Identifikator
  </sql-query>
  <sql-query name="MenoFOGet">
    <return class="Ditec.RIS.RFO.Dol.Meno"></return>
    exec FO.MENO_FYZICKA_OSOBA_GET :FYZICKA_OSOBA_ID
  </sql-query>

</hibernate-mapping>
